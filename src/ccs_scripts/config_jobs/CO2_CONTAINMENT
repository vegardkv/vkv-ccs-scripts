EXECUTABLE   co2_containment

DEFAULT <ROOT_DIR> -1
DEFAULT <OUT_DIR> -1
DEFAULT <CONTAINMENT_POLYGON> -1
DEFAULT <HAZARDOUS_POLYGON> -1
DEFAULT <ZONEFILE> -1
DEFAULT <REGIONFILE> -1
DEFAULT <REGION_PROPERTY> -1
DEFAULT <EGRID> -1
DEFAULT <UNRST> -1
DEFAULT <INIT> -1
DEFAULT <NO_LOGGING> -1
DEFAULT <DEBUG> -1
DEFAULT <RESIDUAL_TRAPPING> -1
DEFAULT <READABLE_OUTPUT> -1
DEFAULT <CONFIG_PLUME_TRACKING> ""

ARGLIST   <CASE>  <CALC_TYPE_INPUT>   "--root_dir" <ROOT_DIR> "--out_dir" <OUT_DIR> "--containment_polygon" <CONTAINMENT_POLYGON> "--hazardous_polygon" <HAZARDOUS_POLYGON> "--zonefile" <ZONEFILE> "--regionfile" <REGIONFILE> "--region_property" <REGION_PROPERTY>  "--egrid"  <EGRID>  "--unrst"  <UNRST> "--init" <INIT> "--no_logging" <NO_LOGGING> "--debug" <DEBUG> "--residual_trapping" <RESIDUAL_TRAPPING> "--readable_output" <READABLE_OUTPUT> "--config_file_inj_wells" <CONFIG_PLUME_TRACKING>

MIN_ARG 2
MAX_ARG 16
