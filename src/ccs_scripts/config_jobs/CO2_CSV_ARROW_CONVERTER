EXECUTABLE   co2_csv_arrow_converter

DEFAULT <ROOT_DIR> -1
DEFAULT <REALIZATION_PATTERN> -1
DEFAULT <KEPT_COLUMNS> -1
DEFAULT <FORCE_ARROW_OVERWRITE> -1
DEFAULT <FORCE_CSV_OVERWRITE> -1

ARGLIST "--root-dir" <ROOT_DIR> "--realization-pattern" <REALIZATION_PATTERN> "--kept-columns" <KEPT_COLUMNS> "--force-arrow-overwrite" <FORCE_ARROW_OVERWRITE> "--force-csv-overwrite" <FORCE_CSV_OVERWRITE>

MIN_ARG 0
MAX_ARG 5
